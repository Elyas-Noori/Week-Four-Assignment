#include<stdio.h>

int main() {
    int num, firstDigit, secondDigit, reversedNum;


    printf("Enter a two-digit number: ");
    scanf("%d", &num);


    firstDigit = num / 10;     
    secondDigit = num % 10;       

   
    reversedNum = secondDigit * 10 + firstDigit;


    printf("The reversal is: %d\n", reversedNum);

    return 0;
}
